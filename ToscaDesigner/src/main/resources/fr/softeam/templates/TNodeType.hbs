node_types:
  {{#each extendedElement}}
  {{owner.name}}.{{name}}:
    description: {{description}}
    derived_from: {{getProperty this "derivedFrom"}}
    metadata:
      targetNamespace: "{{getProperty this "targetNamespace"}}"
      abstract: "{{getProperty this "abstract"}}"
      final: "{{getProperty this "final"}}"
  {{/each}}

